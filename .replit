modules = ["nodejs-20", "web", "bash", "python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Luau Menu Script Preview"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Script Server"

[[workflows.workflow]]
name = "Luau Menu Script Preview"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cat menu.luau"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "Script Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80
